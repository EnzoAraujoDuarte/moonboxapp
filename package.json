{"name":"moonbox","private":true,"workspaces":["frontend","backend"],"scripts":{"dev:frontend":"npm --workspace frontend run dev","dev:backend":"npm --workspace backend run dev","dev":"concurrently -n backend,frontend -c green,blue \"npm:dev:backend\" \"npm:dev:frontend\"","build:frontend":"npm --workspace frontend run build","build:backend":"npm --workspace backend run build","build":"npm run build:backend && npm run build:frontend","start:frontend":"npm --workspace frontend start","start:backend":"npm --workspace backend start","start":"concurrently -n backend,frontend -c green,blue \"npm:start:backend\" \"npm:start:frontend\""},"devDependencies":{"concurrently":"^9.1.2"}}